}

#include<iostream>
using namespace std;

int main(){

for(int i=3;i<51;i++){
        int aux=0;
    for(int j=2;j<i;j++){
        aux=(i%j)+aux;
    if(aux==0){
        cout<<"primo: "<<i<<endl;
    }


    }
}
}


