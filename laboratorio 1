#include <iostream>
using namespace std;
int main(){
    int suma=0;
int k=0;
for(int i=0;i<51;i++){
        k=2*i;
    suma=suma+k;
}
cout<<suma<<endl;
}

#include <iostream>
using namespace std;

int main(){
int a=0;
int b=0;
cout<<"ingrese el primer numero: ";
cin>>a;
cout<<"ingrese el segundo numero: ";
cin>>b;
cout<<"el producto es: "<<a*b<<endl;

}

